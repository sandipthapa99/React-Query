{
    "root": true,
    "extends": [
        "eslint:recommended",
        "prettier"
    ],
    "parser": "@typescript-eslint/parser",
    // "parserOptions": {
    //     "project": "./tsconfig.json",
    //     "sourceType": "module"
    // },
    "plugins": [
        "simple-import-sort",
        "unused-imports",
        "prettier"
    ],
    "rules": {
        "simple-import-sort/imports": "error",
        "simple-import-sort/exports": "error",
        "prettier/prettier": "error",
    }
}
